{
  "name": "polymarket",
  "version": "1.0.0",
  "description": "Polymarket API utilities",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:orders": "ts-node src/index-orders.ts",
    "dev:market": "ts-node src/index-market.ts",
    "dev:both": "concurrently \"npm run dev:orders\" \"npm run api\"",
    "dev:all": "concurrently \"npm run dev:orders\" \"npm run dev:market\" \"npm run api\"",
    "start:orders": "node dist/index-orders.js",
    "start:market": "node dist/index-market.js",
    "start:both": "concurrently \"npm run start:orders\" \"npm run start:market\"",
    "api": "ts-node src/api-server.ts",
    "api:prod": "node dist/api-server.js",
    "dev:all:prod": "concurrently \"npm run start\" \"npm run api:prod\"",
    "analyze": "ts-node src/analyze.ts",
    "analyze-price": "ts-node src/analyze-price.ts"
  },
  "keywords": [
    "polymarket",
    "clob",
    "api"
  ],
  "author": "",
  "dependencies": {
    "axios": "^1.13.2",
    "express": "^5.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@types/xlsx": "^0.0.35",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18"
  }
}
